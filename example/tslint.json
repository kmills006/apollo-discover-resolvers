{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-config-airbnb"],
  "jsRules": {},
  "rules": {
    "import-name": false,
    "no-console": false,
    "object-literal-sort-keys": [true, "shorthand-first"],
    "prefer-array-literal": false,
    "variable-name": [true, "allow-pascal-case"]
  },
  "rulesDirectory": []
}
