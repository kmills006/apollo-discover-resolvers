{
  "name": "apollo-discover-resolvers",
  "version": "1.0.2",
  "description": "Automagically discover your GraphQL resolvers",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Kristy Miller <kristy@kmills.co>",
  "repository": {
    "type": "git",
    "url": "https://github.com/kmills006/apollo-discover-resolvers.git"
  },
  "scripts": {
    "build": "rm -rf dist/ && tsc",
    "dev": "tsc-watch --onSuccess \"node dist/index.js\"",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "prepublishOnly": "yarn build",
    "start": "ts-node src/index.ts"
  },
  "keywords": [
    "apollo",
    "resolvers",
    "graphql"
  ],
  "license": "MIT",
  "dependencies": {
    "app-root-path": "^2.1.0",
    "glob": "^7.1.3",
    "graphql": "^14.0.2",
    "graphql-tools": "^4.0.0",
    "pascal-case": "^2.0.1",
    "path": "^0.12.7",
    "pluralize": "^7.0.0",
    "typescript": "^3.1.1"
  },
  "devDependencies": {
    "@types/app-root-path": "^1.2.4",
    "@types/glob": "^7.1.1",
    "@types/graphql": "^14.0.1",
    "@types/node": "^10.11.4",
    "@types/pascal-case": "^1.1.2",
    "@types/pluralize": "^0.0.29",
    "ts-node": "^7.0.1",
    "tsc-watch": "^1.0.29",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.0"
  }
}
